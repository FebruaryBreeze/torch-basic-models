{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "title": "res_net_config",
  "description": "ResNet Model Config",
  "type": "object",
  "properties": {
    "type": {
      "type": "string"
    },
    "layers_list": {
      "type": "array",
      "items": {
        "type": "integer"
      },
      "default": [3, 4, 6, 3]
    },
    "stride_list": {
      "type": "array",
      "items": {
        "type": "integer"
      },
      "default": [2, 2, 1, 2, 2, 2]
    },
    "feature_dim": {
      "type": "integer",
      "default": 1000
    },
    "dropout_ratio": {
      "type": "number",
      "default": 0.0
    }
  },
  "additionalProperties": false
}
